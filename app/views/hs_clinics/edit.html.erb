<% provide(:title, "Editar clinica") %>
<h1>Actualizar clinica</h1>

<div class="row">
	<div class="span6 offset3">
		<%= form_for(@hs_clinic) do |f| %>
			<%= render 'shared/error_messages', object: f.object %>

			<%= f.label :name %>
			<%= f.text_field :name %>

			<%= f.label :address %>
			<%= f.text_field :address %>

			<%= f.label :country %>
			<%= f.text_field :country %>

			<%= f.label :hs_state_id %>
			<%= f.text_field :hs_state_id %>

			<%= f.label :township %>
			<%= f.text_field :township %>

			<%= f.label :phone %>
			<%= f.text_field :phone %>

			<%= f.label :email %>
			<%= f.text_field :email %>

			<%= f.label :lat %>
			<%= f.text_field :lat%>

			<%= f.label :lng %>
			<%= f.text_field :lng %>

			<%= f.submit "Guardar cambios", class: "btn btn-large" %>
		<% end %>
	</div>
</div>